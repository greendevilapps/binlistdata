        },
        {
            "name": "bank_name",
            "constraints": { "required": false }
        },
        {
            "name": "bank_url",
            "constraints": { 
              "required": false,
              "pattern": "\\A([a-z0-9]+([\-\.]{1}[a-z0-9]+)*\.[a-z]{2,5}|)\\Z"
            }
        },
        {
            "name": "bank_phone",
            "constraints": { "required": false }
        },
        {
            "name": "bank_city",
            "constraints": { "required": false }
        }
    ]
